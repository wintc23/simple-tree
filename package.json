{
  "name": "simple-vue-tree",
  "version": "0.1.11",
  "description": "A simple tree",
  "keywords": [
    "tree",
    "vue-tree"
  ],
  "author": "Lushenggang <lushg-tcxg@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lushenggang/simple-tree"
  },
  "main": "dist/lib/simple-tree.common.js",
  "unpkg": "dist//lib/simple-tree.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name lib/simple-tree ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "yarn build-bundle"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "iview": "^3.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
